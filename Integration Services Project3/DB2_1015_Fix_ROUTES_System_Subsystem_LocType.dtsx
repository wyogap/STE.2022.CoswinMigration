      DTS:DTSID="{297B2074-54AB-4610-A74B-FF4F59D2E440}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update Routes"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{0BC696F0-EC0B-4459-B510-122B6C034F5D}"
          SQLTask:SqlStatementSource="MERGE INTO MAXIMO.ROUTES AS A&#xA;USING (&#xA; WITH DATA AS (&#xA;  SELECT r.routesid, r.route, rs.assetnum &#xA;   , b.ste_system, b.ste_subsystem&#xA;   , b.location, l.TYPE AS locationtype&#xA;  FROM maximo.ROUTES r &#xA;  JOIN MAXIMO.ROUTE_STOP rs ON rs.route=r.route&#xA;  JOIN maximo.ASSET b ON b.assetnum=rs.assetnum&#xA;  LEFT JOIN maximo.LOCATIONS l ON l.location=b.location&#xA;  ORDER BY r.route&#xA; ),&#xA; SYSTEMCNT AS (&#xA;  SELECT routesid, count(DISTINCT ste_system) AS cnt, max(ste_system) AS ste_system&#xA;  FROM DATA&#xA;  WHERE ste_system IS NOT null&#xA;  GROUP BY routesid&#xA; ),&#xA; SUBSYSTEMCNT AS (&#xA;  SELECT routesid, count(DISTINCT ste_subsystem) AS cnt, max(ste_subsystem) AS ste_subsystem&#xA;  FROM DATA&#xA;  WHERE ste_subsystem IS NOT null&#xA;  GROUP BY routesid&#xA; ),&#xA; LOCTYPECNT AS (&#xA;  SELECT routesid, count(DISTINCT locationtype) AS cnt, max(locationtype) AS locationtype&#xA;  FROM DATA&#xA;  WHERE locationtype IS NOT null&#xA;  GROUP BY routesid&#xA; )&#xA; SELECT R.ROUTESID, R.ROUTE, B.CNT, C.CNT, D.CNT&#xA;  , CASE WHEN COALESCE(B.CNT,0)=1 THEN B.STE_SYSTEM ELSE NULL END AS STE_SYSTEM&#xA;  , CASE WHEN COALESCE(C.CNT,0)=1 THEN C.STE_SUBSYSTEM ELSE NULL END AS STE_SUBSYSTEM&#xA;  , CASE WHEN COALESCE(D.CNT,0)=1 THEN D.LOCATIONTYPE ELSE NULL END AS LOCATIONTYPE&#xA;  , R.STE_SYSTEM AS ORIG_SYSTEM&#xA;  , R.STE_SUBSYSTEM AS ORIG_SUBSYSTEM&#xA;  , R.STE_LOCTYPE AS ORIG_LOCTYPE&#xA; FROM MAXIMO.ROUTES R&#xA; LEFT JOIN SYSTEMCNT B ON B.ROUTESID=R.ROUTESID&#xA; LEFT JOIN SUBSYSTEMCNT C ON C.ROUTESID=R.ROUTESID&#xA; LEFT JOIN LOCTYPECNT D ON D.ROUTESID=R.ROUTESID&#xA; ORDER BY R.route&#xA;) B ON B.ROUTESID=A.ROUTESID&#xA;WHEN MATCHED THEN&#xA;UPDATE SET &#xA; ste_system=COALESCE(B.ste_system,B.ORIG_SYSTEM),&#xA; ste_subsystem=COALESCE(B.ste_subsystem,B.ORIG_SUBSYSTEM),&#xA; ste_loctype=COALESCE(B.locationtype,B.ORIG_LOCTYPE)&#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
