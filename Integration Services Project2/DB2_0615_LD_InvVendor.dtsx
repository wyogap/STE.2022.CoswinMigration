              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName">"COSWIN"."STE_TEST_TABLE"</property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT *
FROM (
	SELECT 
		si.PK_SUPL_ITEMS 
		, rmk.PK_SUPL_RMK 
		, rmk.S_SUPLITEM_RMK 
		, s.SUPL_CD 
		, rmk.SUPL_TXT as RAW_TEXT
		, n.NSITEM_CD
		, i.ITEM_CD 
		--, NULL as DEV_TEXT 
		, ROW_NUMBER() OVER (PARTITION BY si.PK_SUPL_ITEMS ORDER BY rmk.PK_SUPL_RMK) rn
	FROM COSWIN.SUPL_RMK rmk  
	JOIN COSWIN.SUPL_ITEMS si ON si.PK_SUPL_ITEMS = rmk.S_SUPLITEM_RMK 
	LEFT JOIN COSWIN.SUPPLIER_ s ON s.PK_SUPPLIER_=si.S_SUPL_ITEM 
	LEFT JOIN coswin.NSITEM n ON n.PK_NSITEM=si.S_NSITEM_SUPL 
	LEFT JOIN coswin.ITEM_ i ON i.PK_ITEM_=si.S_ITEM_SUPL 
	WHERE rmk.S_SUPLITEM_RMK IS NOT NULL
) x 
WHERE x.rn=1
ORDER BY x.PK_SUPL_ITEMS</property>
