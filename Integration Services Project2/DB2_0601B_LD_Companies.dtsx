    {
        rowCount++;
        Row.RowNo = rowCount;

        if (Row.RAWTEXT.IsNull)
        {
            Row.HasFile = 0;
            Row.LDText = null;
            return;
        }
        
        Int32 blobLength = (Int32)Row.RAWTEXT.Length;
        byte[] blobData = null;
        string str = null;

        Row.Length = blobLength;

        try
