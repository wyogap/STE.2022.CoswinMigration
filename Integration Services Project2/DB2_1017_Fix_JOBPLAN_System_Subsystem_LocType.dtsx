    <DTS:Executable
      DTS:refId="Package\Update JobPan from JPAssetsPLink"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{B0470CFA-4CC3-4FE8-BEBF-AAD938E07503}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Update JobPan from JPAssetsPLink"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{0BC696F0-EC0B-4459-B510-122B6C034F5D}"
          SQLTask:SqlStatementSource="MERGE INTO MAXIMO.JOBPLAN AS A&#xA;USING (&#xA;&#x9;WITH DATA AS (&#xA;&#x9;&#x9;SELECT j.jobplanid, j.jpnum, B.ASSETNUM&#xA;&#x9;&#x9;&#x9;, B.ste_system, B.ste_subsystem, C.type AS locationtype, d.maint_entity AS maintentity&#xA;&#x9;&#x9;FROM maximo.jobplan j &#xA;&#x9;&#x9;JOIN MAXIMO.JPASSETSPLINK A ON A.JPNUM=J.JPNUM&#xA;&#x9;&#x9;JOIN MAXIMO.ASSET B ON B.ASSETNUM=A.ASSETNUM&#xA;&#x9;&#x9;LEFT JOIN MAXIMO.LOCATIONS C ON C.LOCATION=B.LOCATION &#xA;&#x9;&#x9;LEFT JOIN MIGRATION.SBST_DEPT_ENTITY_MAPPING d ON d.coswin_entity=b.ste_cswnauthority&#xA;&#x9;&#x9;WHERE B.ste_system IS NOT NULL OR B.ste_subsystem IS NOT NULL or C.type IS NOT NULL&#xA;&#x9;&#x9;&#x9;OR d.maint_entity IS NOT NULL &#xA;&#x9;&#x9;ORDER BY j.jobplanid&#xA;&#x9;),&#xA;&#x9;SYSTEMCNT AS (&#xA;&#x9;&#x9;SELECT jobplanid, count(DISTINCT ste_system) AS cnt, max(ste_system) AS ste_system&#xA;&#x9;&#x9;FROM DATA&#xA;&#x9;&#x9;WHERE ste_system IS NOT null&#xA;&#x9;&#x9;GROUP BY jobplanid&#xA;&#x9;),&#xA;&#x9;SUBSYSTEMCNT AS (&#xA;&#x9;&#x9;SELECT jobplanid, count(DISTINCT ste_subsystem) AS cnt, max(ste_subsystem) AS ste_subsystem&#xA;&#x9;&#x9;FROM DATA&#xA;&#x9;&#x9;WHERE ste_subsystem IS NOT null&#xA;&#x9;&#x9;GROUP BY jobplanid&#xA;&#x9;),&#xA;&#x9;LOCTYPECNT AS (&#xA;&#x9;&#x9;SELECT jobplanid, count(DISTINCT locationtype) AS cnt, max(locationtype) AS locationtype&#xA;&#x9;&#x9;FROM DATA&#xA;&#x9;&#x9;WHERE locationtype IS NOT null&#xA;&#x9;&#x9;GROUP BY jobplanid&#xA;&#x9;),&#xA;&#x9;MAINTENTITY AS (&#xA;&#x9;&#x9;SELECT jobplanid, count(DISTINCT maintentity) AS cnt, max(maintentity) AS maintentity&#xA;&#x9;&#x9;FROM DATA&#xA;&#x9;&#x9;WHERE maintentity IS NOT null&#xA;&#x9;&#x9;GROUP BY jobplanid&#xA;&#x9;)&#xA;&#x9;SELECT J.JOBPLANID, J.JPNUM, B.CNT, C.CNT, D.CNT&#xA;&#x9;&#x9;, CASE WHEN COALESCE(B.CNT,0)=1 THEN B.STE_SYSTEM ELSE NULL END AS STE_SYSTEM&#xA;&#x9;&#x9;, CASE WHEN COALESCE(C.CNT,0)=1 THEN C.STE_SUBSYSTEM ELSE NULL END AS STE_SUBSYSTEM&#xA;&#x9;&#x9;, CASE WHEN COALESCE(D.CNT,0)=1 THEN D.LOCATIONTYPE ELSE NULL END AS LOCATIONTYPE&#xA;&#x9;&#x9;, CASE WHEN COALESCE(E.CNT,0)=1 THEN E.MAINTENTITY ELSE NULL END AS MAINTENTITY&#xA;&#x9;&#x9;, J.STE_SYSTEM AS OLD_SYSTEM&#xA;&#x9;&#x9;, J.STE_SUBSYSTEM AS OLD_SUBSYSTEM&#xA;&#x9;&#x9;, J.ste_loctype AS OLD_LOCATIONTYPE&#xA;&#x9;&#x9;, CASE WHEN J.ste_maintentity='NEL' THEN NULL ELSE J.ste_maintentity END AS OLD_MAINTENTITY&#xA;&#x9;FROM MAXIMO.JOBPLAN J&#xA;&#x9;LEFT JOIN SYSTEMCNT B ON B.JOBPLANID=J.JOBPLANID&#xA;&#x9;LEFT JOIN SUBSYSTEMCNT C ON C.JOBPLANID=J.JOBPLANID&#xA;&#x9;LEFT JOIN LOCTYPECNT D ON D.JOBPLANID=J.JOBPLANID&#xA;&#x9;LEFT JOIN MAINTENTITY E ON E.JOBPLANID=J.JOBPLANID&#xA;&#x9;-- WHERE B.CNT=1 OR C.CNT=1 OR D.CNT=1 OR E.CNT=1&#xA;&#x9;ORDER BY J.JOBPLANID&#xA;) B ON B.JOBPLANID=A.JOBPLANID&#xA;WHEN MATCHED THEN&#xA;UPDATE SET &#xA;&#x9;ste_system=COALESCE(B.ste_system, B.OLD_SYSTEM),&#xA;&#x9;ste_subsystem=COALESCE(B.ste_subsystem, B.OLD_SUBSYSTEM),&#xA;&#x9;ste_loctype=COALESCE(B.locationtype, B.OLD_LOCATIONTYPE),&#xA;&#x9;ste_maintentity=COALESCE(B.maintentity, B.OLD_MAINTENTITY)&#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
