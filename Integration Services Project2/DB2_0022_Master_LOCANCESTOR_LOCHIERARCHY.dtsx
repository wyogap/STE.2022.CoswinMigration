              description="OLE DB Source"
              name="OLE DB Source"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT A.LOCATIONSID, A.LOCATION, C.LOCATION AS PR_LOCATION, A.SYSTEM
	, A.LOCATIONCODE, A.PARENT
	, A.DESCRIPTION, A.TYPE, A.STE_MIGRATIONID, A.STE_MIGRATIONSOURCE
	, b.max_id + ROW_NUMBER() OVER (ORDER BY a.locationcode) AS ID
FROM (
	SELECT
		a.LOCATIONSID, a.LOCATION, a.locationcode, a.SYSTEM, a.cnt AS level
		, CASE WHEN a.last_mark=0 THEN 'NEL' 
			   ELSE substring(a.locationcode, 1, a.last_mark-1) 
		  END AS parent
		, A.DESCRIPTION, A.TYPE, A.STE_MIGRATIONID, A.STE_MIGRATIONSOURCE
	FROM (
		SELECT LOCATIONSID, LOCATION, STE_LOCCODE AS locationcode
			, CASE WHEN TYPE IN ('DEPOT', 'DEPTRK') THEN 'DEPOT'
				   WHEN TYPE = 'STATION' THEN 'STATION'
				   WHEN TYPE = 'TRAIN' THEN 'TRAIN'
				   WHEN TYPE = 'TUNTRK' THEN 'TUNTRK'
				   WHEN TYPE IN ('VIADUCT', 'VIADTRK') THEN 'VIADUCT'
			  END AS SYSTEM
			, LENGTH(STE_LOCCODE) - LENGTH(REPLACE(STE_LOCCODE, '/', '')) AS cnt
			, LOCATE_IN_STRING(STE_LOCCODE, '/', 1) AS first_mark 
			, LOCATE_IN_STRING(STE_LOCCODE, '/', -1) AS last_mark
			, DESCRIPTION, TYPE, STE_MIGRATIONID, STE_MIGRATIONSOURCE
		FROM MAXIMO.LOCATIONS
		WHERE STE_MIGRATIONID IS NOT NULL
			AND TYPE IN ('DEPOT', 'DEPTRK', 'STATION', 'TRAIN', 'TUNTRK', 'VIADUCT', 'VIADTRK')
	) AS a
) a
JOIN (
	SELECT COALESCE(max(LOCANCESTORID), 0) AS max_id FROM maximo.LOCANCESTOR l
) AS b ON 1=1
LEFT JOIN MAXIMO.LOCATIONS C ON COALESCE(C.STE_LOCCODE,C.LOCATION)=A.PARENT</property>
