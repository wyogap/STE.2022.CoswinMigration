          SQLTask:SqlStatementSource="-- TOP LEVEL&#xA;INSERT INTO MAXIMO.LOCANCESTOR(&#xA;&#x9;LOCATION, ANCESTOR, SYSTEMID, SITEID, ORGID, LOCANCESTORID, STE_MIGRATIONID, STE_MIGRATIONDATE&#xA;)&#xA;SELECT 'NEL' AS LOCATION, 'NEL' AS ANCESTOR, A.N AS SYSTEMID, 'NEL' AS SITEID, 'SBST' AS ORGID&#xA;&#x9;, B.MAX_ID + ROW_NUMBER() OVER (ORDER BY A.N) AS LOCANCESTORID&#xA;&#x9;, 1 AS STE_MIGRATIONID, ADD_HOURS(CURRENT_TIMESTAMP, 8) AS STE_MIGRATIONDATE&#xA;FROM (&#xA;&#x9;VALUES('DEPOT'),('STATION'),('TUNNEL'),('VIADUCT'),('TRAIN')&#xA;) AS A(n)&#xA;LEFT JOIN (&#xA;&#x9;select ifnull(max(LOCANCESTORID),1) as max_id from MAXIMO.LOCANCESTOR&#xA;) B ON 1=1 &#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
