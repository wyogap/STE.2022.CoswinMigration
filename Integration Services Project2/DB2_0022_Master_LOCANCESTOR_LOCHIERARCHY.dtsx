          SQLTask:SqlStatementSource="INSERT INTO MAXIMO.LOCANCESTOR(&#xA; LOCATION, ANCESTOR, SYSTEMID, SITEID, ORGID, LOCANCESTORID, STE_MIGRATIONID, STE_MIGRATIONDATE&#xA;)&#xA;SELECT A.LOCATION, A.LOCATION AS ANCESTOR, A.SYSTEM AS SYSTEMID, 'NEL' AS SITEID, 'SBST' AS ORGID&#xA; , B.MAX_ID + ROW_NUMBER() OVER (ORDER BY A.SYSTEM, A.LOCATIONSID) AS LOCANCESTORID&#xA; , A.LOCATIONSID AS STE_MIGRATIONID, ADD_HOURS(CURRENT_TIMESTAMP, 8) AS STE_MIGRATIONDATE&#xA;FROM (&#xA;&#x9;SELECT LOCATION, STE_LOCCODE AS locationcode&#xA;&#x9;&#x9;, CASE WHEN TYPE IN ('DEPOT', 'DEPTRK') THEN 'DEPOT'&#xA;&#x9;&#x9;&#x9;   WHEN TYPE = 'STATION' THEN 'STATION'&#xA;&#x9;&#x9;&#x9;   WHEN TYPE = 'TRAIN' THEN 'TRAIN'&#xA;&#x9;&#x9;&#x9;   WHEN TYPE = 'TUNTRK' THEN 'TUNTRK'&#xA;&#x9;&#x9;&#x9;   WHEN TYPE IN ('VIADUCT', 'VIADTRK') THEN 'VIADUCT'&#xA;&#x9;&#x9;  END AS SYSTEM&#xA;&#x9;&#x9;, LOCATIONSID&#xA;&#x9;FROM MAXIMO.LOCATIONS&#xA;&#x9;WHERE STE_MIGRATIONID IS NOT NULL&#xA;&#x9;&#x9;AND TYPE IN ('DEPOT', 'DEPTRK', 'STATION', 'TRAIN', 'TUNTRK', 'VIADUCT', 'VIADTRK')&#xA;) AS A&#xA;LEFT JOIN (&#xA; select ifnull(max(LOCANCESTORID),1) as max_id from MAXIMO.LOCANCESTOR&#xA;) B ON 1=1 &#xA;;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
