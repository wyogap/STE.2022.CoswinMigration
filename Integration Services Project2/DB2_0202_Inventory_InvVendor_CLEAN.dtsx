              <properties>
                <property
                  dataType="System.String"
                  description="The name of the table to be fetched."
                  expressionType="Notify"
                  name="TableName">"COSWIN"."STE_TEST_TABLE"</property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=16.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT 
	si.PK_SUPL_ITEMS
	, si.S_ITEM_REF
	, si.S_ITEM_UNIT
	, si.S_LAT_UNIT_RATE
	, si.S_L_DT_PO
	, si.S_L_PO
	, CASE WHEN ns.PK_NSITEM IS NOT NULL THEN ns.NSITEM_CD ELSE it.ITEM_CD END AS ITEM_CD
	, CASE WHEN ns.PK_NSITEM IS NOT NULL THEN ns.NSITEM_SHORT ELSE it.ITEM_SHORT END AS ITEM_DS
	, s.SUPL_CD 
	, it.PK_ITEM_ 
                , it.I_VAT_CD
	, IF.AVG_LEAD_TIME 
	, IF.MIN_LEAD_TIME 
	, IF.MAX_LEAD_TIME 
	, it.PLAN_DAYS
FROM COSWIN.SUPL_ITEMS si
LEFT JOIN COSWIN.ITEM_ it ON it.PK_ITEM_ = si.S_ITEM_SUPL  
LEFT JOIN ITEM_INFO IF ON IF.S_ITEM_INFO = it.PK_ITEM_ 
LEFT JOIN COSWIN.NSITEM ns ON ns.PK_NSITEM = si.S_NSITEM_SUPL  
LEFT JOIN COSWIN.SUPPLIER_ s ON s.PK_SUPPLIER_ = si.S_SUPL_ITEM 
-- must be valid item
WHERE (CASE WHEN ns.PK_NSITEM IS NOT NULL THEN ns.NSITEM_CD ELSE it.ITEM_CD END) IS NOT NULL
--and si.PK_SUPL_ITEMS &gt; 197410
order by si.PK_SUPL_ITEMS</property>
