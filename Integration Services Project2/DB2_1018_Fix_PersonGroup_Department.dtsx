          SQLTask:Connection="{0BC696F0-EC0B-4459-B510-122B6C034F5D}"
          SQLTask:SqlStatementSource="MERGE INTO MAXIMO.PERSONGROUP AS A&#xA;USING (&#xA;&#x9;SELECT p.PERSONGROUPID, p.PERSONGROUP, coalesce(m.dept, 'NEL') AS DEPARTMENT &#xA;&#x9;&#x9;, ROW_NUMBER() OVER (PARTITION BY p.PERSONGROUP ORDER BY m.ID) AS RN&#xA;&#x9;FROM maximo.PERSONGROUP p &#xA;&#x9;LEFT JOIN MIGRATION.SBST_DEPT_ENTITY_MAPPING m ON m.maint_entity=p.persongroup&#xA;) AS B ON B.PERSONGROUPID=A.PERSONGROUPID AND B.RN=1&#xA;WHEN MATCHED THEN&#xA;UPDATE SET &#xA;&#x9;ste_department=B.DEPARTMENT&#xA;;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
