/// <summary>
/// This is the class to which to add your code.  Do not change the name, attributes, or parent
/// of this class.
/// </summary>
[Microsoft.SqlServer.Dts.Pipeline.SSISScriptComponentEntryPointAttribute]
public class ScriptMain : UserComponent
{
    // Create the variables that will track the running values
    int rowCount;

    byte[] MARKER_ARIAL = new byte[] { 0x00, 0x02, 0x00, 0x01, 0x00, 0x03, 0x00, 0xf5, 0x00, 0x00, 0x00, 0x90, 0x01, 0x00, 0x00, 0x00, 0x22, 0x41, 0x72, 0x69, 0x61, 0x6c };
    byte[] MARKER_OLE = new byte[] { 0x4f, 0x4c, 0x45, 0x49, 0x74, 0x65, 0x6d };

    public override void PreExecute()
    {
        base.PreExecute();

        // Initialize the running values
        rowCount = 0;
    }

    public override void PostExecute()
    {
        base.PostExecute();
    }

    public override void Input0_ProcessInputRow(Input0Buffer Row)
    {
        rowCount++;
        Row.RowNo = rowCount;

        if (Row.RAWTEXT.IsNull)
        {
            Row.HasFile = 0;
            Row.LDText = null;
            return;
        }
        
        Int32 blobLength = (Int32)Row.RAWTEXT.Length;
        byte[] blobData = null;
        string str = null;

        Row.Length = blobLength;

        try
        {
            blobData = Row.RAWTEXT.GetBlobData(0, blobLength);
        }
        catch(Exception ex)
        {
            Row.HasFile = 1;
            Row.LDText = "Conversion Error";
            return;
        }

        if (blobLength == 67 && SearchArray(MARKER_ARIAL, blobData))
        {
            Row.HasFile = 0;
            Row.LDText = null;
            return;
        }

        if (blobLength > 8000 || SearchArray(MARKER_OLE, blobData))
        {
            Row.HasFile = 1;
            Row.LDText = "Embedded Attachment";
            return;
        }

        try
        {
            str = System.Text.Encoding.Default.GetString(blobData);
            //str = Regex.Replace(str, @"\r\n?|\n", "");
        }
        catch (Exception ex)
        {
            Row.HasFile = 1;
            Row.LDText = "Conversion Error";
            return;
        }

        Row.HasFile = 0;
        Row.LDText = str;

        return;
    }

    private static bool SearchArray(byte[] needle, byte[] haystack)
    {
        int[] foundIndices = new int[needle.Length];

        int found = 0;

        for (int i = 0; i < haystack.Length; i++)
        {

            if (needle[found] == haystack[i])
            {
                foundIndices[found++] = i;

                if (found == needle.Length)
                    return true;
            }
            else
            {
                i -= found;   // Re-evaluate from the start of the found sentence + 1
                found = 0;    // Gap found, reset, maybe later in the haystack another occurrance of needle[0] is found
                continue;
            }
        }

        return false;
    }
}
